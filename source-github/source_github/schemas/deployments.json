{
  "$schema": "https://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": ["id"],
  "properties": {
    "url": {
      "type": ["null", "string"]
    },
    "id": {
      "type": "integer"
    },
    "node_id": {
      "type": ["null", "string"]
    },
    "task": {
      "type": ["null", "string"]
    },
    "original_environment": {
      "type": ["null", "string"]
    },
    "environment": {
      "type": ["null", "string"]
    },
    "description": {
      "type": ["null", "string"]
    },
    "created_at": {
      "type": "string",
      "format": "date-time"
    },
    "updated_at": {
      "type": "string",
      "format": "date-time"
    },
    "statuses_url": {
      "type": ["null", "string"]
    },
    "repository_url": {
      "type": ["null", "string"]
    },
    "creator": {
      "$ref": "user.json"
    },
    "sha": {
      "type": ["null", "string"]
    },
    "ref": {
      "type": ["null", "string"]
    },
    "payload": {
      "oneOf": [
        {
          "type": "object",
          "properties": {},
          "additionalProperties": true
        },
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ]
    },
    "transient_environment": {
      "type": ["null", "boolean"]
    },
    "production_environment": {
      "type": ["null", "boolean"]
    },
    "performed_via_github_app": {
        "type": ["null", "object"],
        "properties": {
            "description": {
                "type": ["null", "string"]
            },
            "events": {
                "type": ["array", "null"]
            },
            "external_url": {
                "type": ["string", "null"]
            },
            "html_url": {
                "type": ["string", "null"]
            },
            "id": {
                "type": ["integer", "null"]
            },
            "name": {
                "type": ["string", "null"]
            },
            "node_id": {
                "type": ["string", "null"]
            },
            "owner": {
                "type": ["object", "null"],
                "properties": {
                    "id": { "type": ["string", "integer", "null"], "format": "integer" },
                    "type": {
                        "type": ["string", "null"]
                    }
                }
            },
            "slug": {
                "type": ["string", "null"]
            },
            "updated_at": {
                "type": ["string", "null"],
                "format": "date-time"
            }
        }
    },
    "repository": {
      "type": "string"
    }
  }
}
