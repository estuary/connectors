[
  {
    "protocol": 3032023,
    "configSchema": {
      "$defs": {
        "Advanced": {
          "properties": {
            "skip_flashback_retention_checks": {
              "default": false,
              "description": "Skip Flashback retention checks. Use this cautiously as we cannot guarantee consistency if Flashback retention is not sufficient.",
              "title": "Skip Flashback Retention Checks",
              "type": "boolean"
            },
            "default_interval": {
              "default": "PT1M",
              "description": "Default interval between updates for all resources. Can be overwritten by each resource.",
              "format": "duration",
              "title": "Default Interval",
              "type": "string"
            }
          },
          "title": "Advanced",
          "type": "object"
        },
        "BasicAuth": {
          "properties": {
            "credentials_title": {
              "const": "Username & Password",
              "enum": [
                "Username & Password"
              ],
              "title": "Credentials Title",
              "type": "string"
            },
            "username": {
              "title": "Username",
              "type": "string"
            },
            "password": {
              "secret": true,
              "title": "Password",
              "type": "string"
            }
          },
          "required": [
            "credentials_title",
            "username",
            "password"
          ],
          "title": "BasicAuth",
          "type": "object"
        },
        "Wallet": {
          "properties": {
            "credentials_title": {
              "const": "Wallet",
              "enum": [
                "Wallet"
              ],
              "title": "Credentials Title",
              "type": "string"
            },
            "username": {
              "title": "Username",
              "type": "string"
            },
            "password": {
              "secret": true,
              "title": "Password",
              "type": "string"
            },
            "tnsnames": {
              "description": "tnsnames.ora file from the wallet zip file",
              "multiline": true,
              "title": "tnsnames.ora file",
              "type": "string"
            },
            "ewallet": {
              "description": "ewallet.pem file from the wallet zip file",
              "multiline": true,
              "secret": true,
              "title": "ewallet.pem file",
              "type": "string"
            },
            "wallet_password": {
              "description": "Wallet's password",
              "secret": true,
              "title": "Password",
              "type": "string"
            }
          },
          "required": [
            "credentials_title",
            "username",
            "password",
            "tnsnames",
            "ewallet",
            "wallet_password"
          ],
          "title": "Wallet",
          "type": "object"
        }
      },
      "properties": {
        "address": {
          "description": "Address",
          "title": "Address",
          "type": "string"
        },
        "credentials": {
          "discriminator": {
            "mapping": {
              "Username & Password": "#/$defs/BasicAuth",
              "Wallet": "#/$defs/Wallet"
            },
            "propertyName": "credentials_title"
          },
          "oneOf": [
            {
              "$ref": "#/$defs/BasicAuth"
            },
            {
              "$ref": "#/$defs/Wallet"
            }
          ],
          "title": "Authentication"
        },
        "advanced": {
          "advanced": true,
          "allOf": [
            {
              "$ref": "#/$defs/Advanced"
            }
          ],
          "description": "Advanced settings for the connector.",
          "title": "Advanced Config"
        }
      },
      "required": [
        "address",
        "credentials"
      ],
      "title": "EndpointConfig",
      "type": "object"
    },
    "resourceConfigSchema": {
      "additionalProperties": false,
      "properties": {
        "name": {
          "description": "Name of this resource",
          "title": "Name",
          "type": "string"
        },
        "interval": {
          "default": "PT0S",
          "description": "Interval between updates for this resource",
          "format": "duration",
          "title": "Interval",
          "type": "string"
        },
        "schema": {
          "default": false,
          "description": "In Oracle tables reside in a schema that points to the user that owns the table.",
          "title": "The schema (the owner) of the table",
          "type": "string"
        }
      },
      "required": [
        "name"
      ],
      "title": "ResourceConfig",
      "type": "object"
    },
    "documentationUrl": "https://go.estuary.dev/source-oracle-batch",
    "resourcePathPointers": [
      "/name"
    ]
  }
]
