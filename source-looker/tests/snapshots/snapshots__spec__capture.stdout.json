[
  {
    "protocol": 3032023,
    "configSchema": {
      "$defs": {
        "OAuth2": {
          "properties": {
            "credentials_title": {
              "const": "OAuth Credentials",
              "default": "OAuth Credentials",
              "title": "Credentials Title",
              "type": "string"
            },
            "client_id": {
              "secret": true,
              "title": "Client Id",
              "type": "string"
            },
            "client_secret": {
              "secret": true,
              "title": "Client Secret",
              "type": "string"
            }
          },
          "required": [
            "client_id",
            "client_secret"
          ],
          "title": "OAuth",
          "type": "object"
        }
      },
      "properties": {
        "subdomain": {
          "description": "The subdomain for your Looker account. For example in https://estuarydemo.cloud.looker.com/folders/home, estuarydemo.cloud.looker.com is the subdomain.",
          "pattern": "^([a-z0-9]+(-[a-z0-9]+)*\\.)+[a-z]{2,}$",
          "title": "Looker Subdomain",
          "type": "string"
        },
        "credentials": {
          "discriminator": {
            "mapping": {
              "OAuth Credentials": "#/$defs/OAuth2"
            },
            "propertyName": "credentials_title"
          },
          "oneOf": [
            {
              "$ref": "#/$defs/OAuth2"
            }
          ],
          "title": "Authentication"
        }
      },
      "required": [
        "subdomain",
        "credentials"
      ],
      "title": "EndpointConfig",
      "type": "object"
    },
    "resourceConfigSchema": {
      "additionalProperties": false,
      "description": "ResourceConfig is a common resource configuration shape.",
      "properties": {
        "_meta": {
          "anyOf": [
            {
              "additionalProperties": true,
              "type": "object"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Meta"
        },
        "name": {
          "description": "Name of this resource",
          "title": "Name",
          "type": "string"
        },
        "interval": {
          "default": "PT0S",
          "description": "Interval between updates for this resource",
          "format": "duration",
          "title": "Interval",
          "type": "string"
        }
      },
      "required": [
        "name"
      ],
      "title": "ResourceConfig",
      "type": "object"
    },
    "documentationUrl": "https://go.estuary.dev/source-looker",
    "resourcePathPointers": [
      "/name"
    ]
  }
]
