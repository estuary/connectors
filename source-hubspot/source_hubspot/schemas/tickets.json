{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": ["object"],
  "properties": {
    "id": {
      "type": "string"
    },
    "createdAt": {
      "type": ["null", "string"],
      "format": "date-time"
    },
    "updatedAt": {
      "type": ["null", "string"],
      "format": "date-time"
    },
    "archived": {
      "type": ["null", "boolean"]
    },
    "contacts": {
      "type": ["null", "array"],
      "items": {
        "type": ["integer", "string", "null"],
        "format": "integer"
      }
    },
    "companies": {
      "type": ["null", "array"],
      "items": {
        "type": ["integer", "string", "null"],
        "format": "integer"
      }
    },
    "deals": {
      "type": ["null", "array"],
      "items": {
        "type": ["null", "string"]
      }
    }
  },
  "required": ["id"]
}
