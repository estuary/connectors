            },
            "access_token": {
              "default": "",
              "secret": true,
              "title": "Access Token",
              "type": "string",
              "x-hidden-field": true
            },
            "client_id": {
              "description": "Client ID from your RingCentral JWT Auth app.",
              "secret": true,
              "title": "Client ID",
