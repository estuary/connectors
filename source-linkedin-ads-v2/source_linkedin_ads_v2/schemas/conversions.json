{
  "$schema": "https://json-schema.org/draft-07/schema#",
  "type": "object",
  "title": "Conversions",
  "required": ["id"],
  "properties": {
    "id": {
      "description": "Unique identifier for the conversion.",
      "type": "integer"
    },
    "lastModified": {
      "description": "The date and time when the user account was last modified",
      "type": ["null", "string"],
      "format": "date-time"
    }
  }
}
