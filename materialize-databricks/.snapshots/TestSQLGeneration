      key1, key2
    FROM 'test-staging-path'
  )
  FILEFORMAT = JSON
  FILES = ('0_load.json')
  FORMAT_OPTIONS ( 'mode' = 'FAILFAST', 'inferTimestamp' = 'true' )
	COPY_OPTIONS ( 'force' = 'true' )
  ;
--- End `a-schema`.target_table copyIntoLoad ---

