--- End "a-schema".target_table createTargetTable ---

--- Begin "Delta Updates" createTargetTable ---
CREATE TABLE IF NOT EXISTS "Delta Updates" (
	theKey VARCHAR(4096),
	nullableKey VARCHAR(4096),
