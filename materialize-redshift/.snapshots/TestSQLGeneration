--- Begin "a-schema".target_table createTargetTable ---
CREATE TABLE IF NOT EXISTS "a-schema".target_table (
	key1 BIGINT,
	"key!2" BOOLEAN,
	nullableKey VARCHAR(4096),
	Camel_Case BIGINT,
	"a Time" TIMESTAMPTZ,
	"array" SUPER,
	lower_case BIGINT,
	value VARCHAR(4096),
	flow_document SUPER
);

