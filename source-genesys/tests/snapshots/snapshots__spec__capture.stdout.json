            "mypurecloud.jp",
            "cac1.pure.cloud",
            "mypurecloud.de",
            "mypurecloud.ie",
            "euw2.pure.cloud",
            "euc2.pure.cloud",
            "mec1.pure.cloud",
            "sae1.pure.cloud",
