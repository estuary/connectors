{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": [
    "_meta",
    "canary",
    "id"
  ],
  "properties": {
    "_meta": {
      "type": "object",
      "required": [
        "file",
        "offset"
      ],
      "properties": {
        "file": {
          "type": "string"
        },
        "offset": {
          "type": "integer"
        }
      }
    },
    "canary": {
      "type": "string"
    },
    "id": {
      "type": "string"
    }
  }
}
